{
  "name": "WBoilerplate",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "typescript": "tsc",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint '*/**/*.{ts,tsx}' --fix -f rdjson .",
    "format": "prettier --write './**/*.{ts,tsx,js,jsx}' --config ./.prettierrc",
    "precommit": "yarn format && git add .",
    "prepare": "husky install",
    "release": "standard-version",
    "postinstall": "patch-package",
    "afterRelease": "export PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git add -A && git commit -m 'build[skip ci]: '$PACKAGE_VERSION && git push https://github.com/thisiswunder/wunder-boilerplate.git --all --force && git push --tags"
  },
  "dependencies": {
    "react": "18.0.0",
    "react-native": "0.69.2",
    "react-native-reanimated": "^2.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/config-conventional": "^17.0.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "changelog-parser": "^2.8.1",
    "commitlint": "^17.0.3",
    "eslint": "^7.32.0",
    "eslint-config-react-native-wcandillon": "^3.8.0",
    "eslint-formatter-rdjson": "^1.0.5",
    "eslint-plugin-reanimated": "^1.2.7",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "patch-package": "^6.4.7",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
