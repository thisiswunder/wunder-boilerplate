{
  "expo": {
    "name": "wunder-boilerplate",
    "slug": "wunder-boilerplate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "is.wunder.boilerplate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "is.wunder.boilerplate"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
